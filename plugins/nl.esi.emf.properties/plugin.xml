            class="nl.esi.emf.properties.util.EPropertyValueConversionDelegate$Factory"
            uri="bundle:/nl.esi.emf.properties/nl.esi.emf.properties.util.EPropertyValueConversionDelegate$Factory">
      </factory>
   </extension>

</plugin>
